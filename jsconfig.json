{
  "compilerOptions": {
    "baseUrl": ".",
"paths": {
  "@/*": ["./*"],
  "@/components/*": ["components/*"],
  "@/types/*": ["types/*"],
  "@/lib/*": ["lib/*"], //any other root-level paths
  "@/_utils/*": ["_utils/*"], //any other root-level paths
},
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ]
}
